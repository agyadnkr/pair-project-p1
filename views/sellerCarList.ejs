<%- include('./partials/header') %> 
<%- include('./partials/nav') %> 
<%- include('./partials/message') %> 

  <h1>Car List</h1>
  <a href="/sell/add">Add Car</a> <br><br>

    <table border="1">
        <tr>
          <th>Brand</th>
          <th>Model</th>

          <th>Year</th>
          <th>Transmission</th>
          <th>Color</th>
          <th>price</th>
          <th>Status</th>
          <th>Details</th>
          <th>Action</th>
        </tr>
    
        <% data.forEach(each => { %>
          <tr>
            <td><%= each.Brand.brandName %></td>
            <td><%= each.model %></td>

            <td><%= each.year %></td>
            <td><%= each.Detail.transmission %></td>
            <td><%= each.color %></td>
            <td><%= each.formatPrice %></td>
            <td><%= each.status %></td>
            <td><a href="/sell/<%= each.id %>/detail">Details</a></td>
            <td>
              
              <a href="/sell/<%= each.id %>/edit">Edit Car</a>
              <a href="/sell/<%= each.id %>/delete">Delete Car</a>
            </td>
          </tr>
          <% }) %>
      </table>  

<%- include('./partials/footer') %> 